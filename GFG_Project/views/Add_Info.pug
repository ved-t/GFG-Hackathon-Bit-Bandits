<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  style
    include ../static/Add_Info.css
  <title>Your Form Title</title>
</head>
<body>

  <form action="/addInfo" method="post">
    <label for="name">Name:</label>
    <input type="text" id="name" name="name" required>

    <label for="drName">DR. Name:</label>
    <input type="text" id="drName" name="drName" required>

    <label for="description">Description:</label>
    <textarea id="description" name="description" rows="4" required></textarea>

    
    <label for="bodyParts">Body Parts:</label>
    <div class="checkbox-group">
      <label><input type="checkbox" name="checkboxes1" value="option-1"> Option 1</label>
      <label><input type="checkbox" name="checkboxes1" value="option-2"> Option 2</label>
      <label><input type="checkbox" name="checkboxes1" value="option-3"> Option 3</label>
      <label><input type="checkbox" name="checkboxes1" value="option-4"> Option 4</label>
      <label><input type="checkbox" name="checkboxes1" value="option-5"> Option 5</label>
      <label><input type="checkbox" name="checkboxes1" value="option-6"> Option 6</label>
      <label><input type="checkbox" name="checkboxes1" value="option-7"> Option 7</label>
    </div>

    
    <label for="symptoms">Symptoms:</label>
    <div class="checkbox-group">
      <label><input type="checkbox" name="checkboxes2" value="option-8"> Option 8</label>
      <label><input type="checkbox" name="checkboxes2" value="option-9"> Option 9</label>
      <label><input type="checkbox" name="checkboxes2" value="option-10"> Option 10</label>
      <label><input type="checkbox" name="checkboxes2" value="option-11"> Option 11</label>
      <label><input type="checkbox" name="checkboxes2" value="option-12"> Option 12</label>
      <label><input type="checkbox" name="checkboxes2" value="option-13"> Option 13</label>
      <label><input type="checkbox" name="checkboxes2" value="option-14"> Option 14</label>
      <label><input type="checkbox" name="checkboxes2" value="option-15"> Option 15</label>
      <label><input type="checkbox" name="checkboxes2" value="option-16"> Option 16</label>
      <label><input type="checkbox" name="checkboxes2" value="option-17"> Option 17</label>
    </div>

    <label for="otherCategory">Other Category:</label>
    <textarea id="otherCategory" name="otherCategory" rows="4"></textarea>

    <label for="date">Date:</label>
    <input type="date" id="date" name="date" required>

    <label for="time">Time:</label>
    <input type="time" id="time" name="time" required>

    <button type="submit">Submit</button>
  </form>

</body>
</html>
